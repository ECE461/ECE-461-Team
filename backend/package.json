{
  "name": "my-package-manager",
  "version": "1.0.0",
  "description": "A package manager that ranks packages based on various factors.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npx ts-node src/index.ts",
    "local": "bash tunnel.sh && npm run start",
    "postlocal": "bash tunnel.sh --close",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "deepclean": "rm -rf node_modules && rm -f package-lock.json && npm cache clean --force",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.675.0",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "es6-promise": "^4.2.8",
    "express": "^4.21.0",
    "isomorphic-git": "^1.27.1",
    "joi": "^17.13.3",
    "jose": "^5.9.6",
    "js-base64": "^3.7.7",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "rimraf": "^6.0.1",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/commander": "^2.12.0",
    "@types/express": "^5.0.0",
    "@types/hapi__joi": "^17.1.14",
    "@types/js-base64": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.6",
    "@types/pg": "^8.11.10",
    "@types/semver": "^7.5.8",
    "@types/sqlite3": "^3.1.11",
    "@types/tar": "^6.1.13",
    "commander": "^12.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
